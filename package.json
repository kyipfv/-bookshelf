{
  "name": "bookshelf",
  "version": "1.0.0",
  "description": "Catalogue your personal library with OCR and genre statistics",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "server:dev": "nodemon server/index.js",
    "client:dev": "cd client && npm run dev",
    "build": "npm run client:build && npm run prisma:generate",
    "client:build": "cd client && npm run build",
    "start": "node server/index.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "test": "vitest",
    "test:run": "vitest run",
    "postinstall": "prisma generate && cd client && npm install"
  },
  "dependencies": {
    "@prisma/client": "^6.13.0",
    "axios": "^1.5.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "multer": "^1.4.5-lts.1",
    "sharp": "^0.32.6",
    "tesseract.js": "^5.0.3",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "concurrently": "^8.2.1",
    "nodemon": "^3.0.1",
    "prisma": "^6.13.0",
    "vitest": "^1.0.4"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
